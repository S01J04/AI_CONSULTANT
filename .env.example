# Firebase Configuration
VITE_apiKey=your-api-key
VITE_authDomain=your-project-id.firebaseapp.com
VITE_databaseURL=https://your-project-id.firebasedatabase.app
VITE_projectId=your-project-id
VITE_storageBucket=your-project-id.appspot.com
VITE_messagingSenderId=your-messaging-sender-id
VITE_appId=your-app-id
VITE_measurementId=your-measurement-id

# External API Configuration
VITE_openAIKey=your-openai-api-endpoint

# reCAPTCHA Configuration (for Firebase App Check)
VITE_recaptchaSiteKey=your-recaptcha-site-key

# Razorpay Configuration (for payment processing)
VITE_razorpayKeyId=your-razorpay-key-id

# Security Configuration
VITE_STORAGE_ENCRYPTION_KEY=your-encryption-key-for-local-storage

# Development Configuration
VITE_USE_EMULATORS=false # Set to 'true' to use Firebase emulators
